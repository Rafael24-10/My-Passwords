{
    "require-dev": {
        "pestphp/pest": "^2.34",
        "mockery/mockery": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/",
            "App\\Models\\": "src/classes/",
            "App\\Controllers\\": "src/classes/",
            "App\\Traits\\": "src/classes/"
        }
    }
}
